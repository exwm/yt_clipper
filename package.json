{
  "name": "yt_clipper",
  "description": "Mark up YouTube videos and quickly generate clipped webms.",
  "author": "exwm",
  "version": "5.34.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/exwm/yt_clipper"
  },
  "homepage": "https://openuserjs.org/scripts/elwm/yt_clipper",
  "source": "src/markup/yt_clipper.ts",
  "targets": {
    "userscript": {
      "distDir": "./dist/js",
      "engines": {
        "browsers": "since 2019 and > 0.5% and not op_mini all and not IE 11"
      }
    }
  },
  "scripts": {
    "lint:py": "just lint-py",
    "test:py": "just test-py",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
  },
  "pre-commit": [
    "precommit-msg",
    "lint:py",
    "test:py"
  ],
  "dependencies": {
    "common-tags": "^1.8.2",
    "d3-drag": "^1.2.5",
    "d3-ease": "^1.0.7",
    "d3-selection": "^1.4.2",
    "dompurify": "^3.1.7",
    "file-saver": "^2.0.5",
    "immer": "^9.0.12",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0"
  },
  "devDependencies": {
    "@types/chart.js": "^2.9.41",
    "@types/common-tags": "^1.8.4",
    "@types/d3-ease": "^1.0.13",
    "@types/dompurify": "^3.0.5",
    "@types/file-saver": "^2.0.7",
    "@types/jszip": "^3.4.1",
    "browserslist": "^4.24.0",
    "commit-and-tag-version": "12.4.4",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.12.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "yarn-audit-fix": "^10.1.1"
  },
  "alias": {
    "jszip": ".parcel-globals/jszip.js",
    "chart.js": ".parcel-globals/chart.js.js"
  }
}
